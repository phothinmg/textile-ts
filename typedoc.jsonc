{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/*"],
  "out": "./docs",
  "includeHierarchySummary": false,
  "headings": {
    "readme": false,
    "document": false
  },
  "favicon": "textile-logo.svg",
  "name": "Textile Ts",
  "typePrintWidth": 60,
  "plugin": ["typedoc-plugin-rename-defaults"],
  "projectDocuments": ["documents/syntax.md"]
}
